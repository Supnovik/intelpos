
server {
	listen 80 default_server;
	listen [::]:80 default_server;

	root /var/www/intelpos.test/public_html;
        index index.php;
	server_name intelpos.test www.intelpos.test;


	
	
location / {
                try_files $uri $uri/ =404;
		rewrite (/[^\./]+)$ $1.php last;
        }

        location ~ \.php$ {
                include snippets/fastcgi-php.conf;
                fastcgi_pass unix:/var/run/php/php7.4-fpm.sock;
        }

        location ~ /\.ht {
                deny all;
        }}






